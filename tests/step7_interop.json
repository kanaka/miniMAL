;; Testing equality

;;; This is different behavior than mal
["if", ["`", ""], 7, 8]
;=>8

["=", ["`", ""], ["`", ""]]
;=>true
["=", ["`", "abc"], ["`", ""]]
;=>false
["=", ["`", ""], ["`", "abc"]]
;=>false
["=", ["`", "abc"], ["`", "def"]]
;=>false
["=", ["list"], ["`", ""]]
;=>false
["=", ["`", ""], ["list"]]
;=>false

;; Testing variable length arguments
[ ["fn", ["&", "more"], ["count", "more"]], 1, 2, 3]
;=>3
[ ["fn", ["&", "more"], ["count", "more"]], 1]
;=>1
[ ["fn", ["&", "more"], ["count", "more"]] ]
;=>0
[ ["fn", ["a", "&", "more"], ["count", "more"]], 1, 2, 3]
;=>2
[ ["fn", ["a", "&", "more"], ["count", "more"]], 1]
;=>0

